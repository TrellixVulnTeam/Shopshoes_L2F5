 <section class="banner-area organic-breadcrumb">
        <div class="container">
            <div class="breadcrumb-banner d-flex flex-wrap align-items-center justify-content-end">
                <div class="col-first">
                    <h1>Checkout</h1>
                    <nav class="d-flex align-items-center">
                        <a href="/">Home<span class="lnr lnr-arrow-right"></span></a>
                        <a href="single-product.html">Checkout</a>
                    </nav>
                </div>
            </div>
        </div>
    </section>

    <section class="checkout_area section_gap">
        <div class="container">
            <div class="billing_details">
                <div class="row">
                    <div class="col-lg-8">
                        <h3>New product information </h3>
                        <form class="row contact_form"  action="/ctsp/store" method="post" novalidate="novalidate">
                            <div class="col-md-6 form-group p_star">
                                 <label for="">Name product</label>
                                <input type="text" class="form-control" id="TenSP" name="TenSP">
                                {{!-- <span class="placeholder" data-placeholder="Name product"></span> --}}
                            </div>
                            <div class="col-md-6 form-group p_star">
                                 <label for="">Cost:</label>
                                <input type="number" class="form-control" id="Gia" name="Gia">
                                {{!-- <span class="placeholder" data-placeholder="Cost"></span> --}}
                            </div>
                            <div class="col-md-12 form-group">
                                <textarea class="form-control" name="GioiThieu" id="GioiThieu" rows="1" placeholder="Gioi thieu San pham"></textarea>
                            </div>

                            <div class="col-md-6 form-group p_star">
                                <label for="">Mau:</label>
                                <select class="country_select" name="TenMau">
                                     {{#each tMau}}
                                    <option value="{{this.TenMau}}">{{this.TenMau}}</option>
                                      {{/each}}
                                </select>
                            </div>
                            <div class="col-md-6 form-group p_star">
                                <label for="">Chat lieu:</label>
                                <select class="country_select" name="ChatLieu">
                                    {{#each tChatLieu}}
                                    <option value="{{this.ChatLieu}}">{{this.ChatLieu}}</option>
                                      {{/each}}
                                </select>
                            </div>
                            <div class="col-md-6 form-group p_star">
                                <label for="">Nha San xuat:</label>
                                <select class="country_select" name="NSX">
                                      {{#each tNSX}}
                                    <option value="{{this.TenNSX}}">{{this.TenNSX}}</option>
                                      {{/each}}
                                </select>
                            </div>
                            <div class="col-md-6 form-group p_star">
                                <label for="">San pham loai:</label>
                                <select class="country_select" name="TenLoai">
                                      {{#each tLoaiSP}}
                                    <option value="{{this.TenLoai}}">{{this.TenLoai}}</option>
